/* 17 */ ScannerDirectives: '%auto_ws_off'^ /* Clipped */;
/* 18 */ ScannerDirectives: '%on'^ /* Clipped */ IdentifierList ScannerStateDirectives;
/* 19 */ ScannerDirectives: '%allow_unmatched'^ /* Clipped */;
