/* 10 */ Declaration: '%grammar_type'^ /* Clipped */ Identifier DeclarationOpt /* Option */;
/* 11 */ Declaration: ScannerDirectives;
/* 12 */ DeclarationOpt /* Option<T>::Some */: GrammarType;
/* 13 */ DeclarationOpt /* Option<T>::None */: ;
