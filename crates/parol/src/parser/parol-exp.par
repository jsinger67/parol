/* 44 */ SimpleTokenOpt /* Option<T>::Some */: ASTControl;
/* 45 */ SimpleTokenOpt /* Option<T>::None */: ;
/* 46 */ TokenWithStates: '<'^ /* Clipped */ IdentifierList '>'^ /* Clipped */ TokenExpression TokenWithStatesOpt /* Option */;
/* 47 */ TokenWithStatesOpt /* Option<T>::Some */: ASTControl;
/* 48 */ TokenWithStatesOpt /* Option<T>::None */: ;
/* 49 */ String: /"(\\.|[^"])*"/;
/* 50 */ RawString: /'(\\.|[^'])*'/;
/* 51 */ Regex: "/(\\.|[^\/])*/";
/* 52 */ Group: '(' Alternations ')';
/* 53 */ Optional: '[' Alternations ']';
/* 54 */ Repeat: '{' Alternations '}';
/* 55 */ NonTerminal: Identifier NonTerminalOpt /* Option */;
/* 56 */ NonTerminalOpt /* Option<T>::Some */: ASTControl;
/* 57 */ NonTerminalOpt /* Option<T>::None */: ;
/* 58 */ Identifier: /[a-zA-Z_][a-zA-Z0-9_]*/;
/* 59 */ ScannerState: '%scanner'^ /* Clipped */ Identifier@state_name '{'^ /* Clipped */ ScannerStateList /* Vec */ '}'^ /* Clipped */;
/* 60 */ ScannerStateList /* Vec<T>::Push */: ScannerDirectives ScannerStateList;
/* 61 */ ScannerStateList /* Vec<T>::New */: ;
