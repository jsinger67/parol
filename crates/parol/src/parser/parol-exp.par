/* 77 */ UserTypeName: Identifier UserTypeNameList /* Vec */;
/* 78 */ UserTypeNameList /* Vec<T>::Push */: DoubleColon^ /* Clipped */ Identifier UserTypeNameList;
/* 79 */ UserTypeNameList /* Vec<T>::New */: ;
