/* 44 */ TokenWithStatesOpt /* Option<T>::Some */: ASTControl;
/* 45 */ TokenWithStatesOpt /* Option<T>::None */: ;
/* 46 */ String: /"(\\.|[^\\])*?"/;
/* 47 */ RawString: /'(\\'|[^'])*?'/;
/* 48 */ Regex: /\u{2F}(\\.|[^\\])*?\u{2F}/;
/* 49 */ Group: '(' Alternations ')';
