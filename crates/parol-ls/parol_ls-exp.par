%start ParolLs
%title "Parol grammar"
%comment "Parol's own grammar enhanced with comment processing"
%line_comment "//"
%block_comment "/\*" "\*/"
%user_type OwnedToken = crate::parol_ls_grammar::OwnedToken

%%

/*  0 */ ParolLs: Prolog GrammarDefinition;
/*  1 */ Prolog: StartDeclaration PrologList /* Vec */ PrologList0 /* Vec */;
/*  2 */ PrologList0 /* Vec<T>::Push */: ScannerState PrologList0;
/*  3 */ PrologList0 /* Vec<T>::New */: ;
/*  4 */ PrologList /* Vec<T>::Push */: Declaration PrologList;
/*  5 */ PrologList /* Vec<T>::New */: ;
/*  6 */ StartDeclaration: "%start" : OwnedToken Identifier;
/*  7 */ Declaration: "%title" : OwnedToken String;
/*  8 */ Declaration: "%comment" : OwnedToken String;
/*  9 */ Declaration: "%user_type" : OwnedToken Identifier "=" : OwnedToken UserTypeName;
/* 10 */ Declaration: '%grammar_type' : OwnedToken LiteralString;
/* 11 */ Declaration: ScannerDirectives;
/* 12 */ ScannerDirectives: "%line_comment" : OwnedToken TokenLiteral;
/* 13 */ ScannerDirectives: "%block_comment" : OwnedToken TokenLiteral TokenLiteral;
/* 14 */ ScannerDirectives: "%auto_newline_off" : OwnedToken;
/* 15 */ ScannerDirectives: "%auto_ws_off" : OwnedToken;
/* 16 */ GrammarDefinition: "%%" : OwnedToken Production GrammarDefinitionList /* Vec */;
/* 17 */ GrammarDefinitionList /* Vec<T>::Push */: Production GrammarDefinitionList;
/* 18 */ GrammarDefinitionList /* Vec<T>::New */: ;
/* 19 */ DoubleColon: "::" : OwnedToken;
/* 20 */ ProductionLHS: Identifier ":" : OwnedToken;
/* 21 */ Production: ProductionLHS Alternations ";" : OwnedToken;
/* 22 */ Alternations: Alternation AlternationsList /* Vec */;
/* 23 */ AlternationsList /* Vec<T>::Push */: '|' : OwnedToken Alternation AlternationsList;
/* 24 */ AlternationsList /* Vec<T>::New */: ;
/* 25 */ Alternation: AlternationList /* Vec */;
/* 26 */ AlternationList /* Vec<T>::Push */: Factor AlternationList;
/* 27 */ AlternationList /* Vec<T>::New */: ;
/* 28 */ Factor: Group;
/* 29 */ Factor: Repeat;
/* 30 */ Factor: Optional;
/* 31 */ Factor: Symbol;
/* 32 */ Symbol: NonTerminal;
/* 33 */ Symbol: SimpleToken;
/* 34 */ Symbol: TokenWithStates;
/* 35 */ Symbol: ScannerSwitch;
/* 36 */ TokenLiteral: String;
/* 37 */ TokenLiteral: LiteralString;
/* 38 */ TokenLiteral: Regex;
/* 39 */ SimpleToken: TokenLiteral SimpleTokenOpt /* Option */;
/* 40 */ SimpleTokenOpt /* Option<T>::Some */: ASTControl;
/* 41 */ SimpleTokenOpt /* Option<T>::None */: ;
/* 42 */ TokenWithStates: "<" : OwnedToken StateList ">" : OwnedToken TokenLiteral TokenWithStatesOpt /* Option */;
/* 43 */ TokenWithStatesOpt /* Option<T>::Some */: ASTControl;
/* 44 */ TokenWithStatesOpt /* Option<T>::None */: ;
/* 45 */ Group: '(' : OwnedToken Alternations ')' : OwnedToken;
/* 46 */ Optional: '[' : OwnedToken Alternations ']' : OwnedToken;
/* 47 */ Repeat: '{' : OwnedToken Alternations '}' : OwnedToken;
/* 48 */ NonTerminal: Identifier NonTerminalOpt /* Option */;
/* 49 */ NonTerminalOpt /* Option<T>::Some */: ASTControl;
/* 50 */ NonTerminalOpt /* Option<T>::None */: ;
/* 51 */ Identifier: /[a-zA-Z_][a-zA-Z0-9_]*/ : OwnedToken;
/* 52 */ String: /"(\\.|[^\\])*?"/ : OwnedToken;
/* 53 */ LiteralString: /'(\\'|[^'])*?'/ : OwnedToken;
/* 54 */ ScannerState: "%scanner" : OwnedToken Identifier '{' : OwnedToken ScannerStateList /* Vec */ '}' : OwnedToken;
/* 55 */ ScannerStateList /* Vec<T>::Push */: ScannerDirectives ScannerStateList;
/* 56 */ ScannerStateList /* Vec<T>::New */: ;
/* 57 */ StateList: Identifier StateListList /* Vec */;
/* 58 */ StateListList /* Vec<T>::Push */: "," : OwnedToken Identifier StateListList;
/* 59 */ StateListList /* Vec<T>::New */: ;
/* 60 */ ScannerSwitch: "%sc" : OwnedToken '(' : OwnedToken ScannerSwitchOpt /* Option */ ')' : OwnedToken;
/* 61 */ ScannerSwitch: "%push" : OwnedToken '(' : OwnedToken Identifier ')' : OwnedToken;
/* 62 */ ScannerSwitch: "%pop" : OwnedToken '(' : OwnedToken ')' : OwnedToken;
/* 63 */ ScannerSwitchOpt /* Option<T>::Some */: Identifier;
/* 64 */ ScannerSwitchOpt /* Option<T>::None */: ;
/* 65 */ ASTControl: CutOperator;
/* 66 */ ASTControl: UserTypeDeclaration;
/* 67 */ CutOperator: '^' : OwnedToken;
/* 68 */ UserTypeDeclaration: ":" : OwnedToken UserTypeName;
/* 69 */ UserTypeName: Identifier UserTypeNameList /* Vec */;
/* 70 */ UserTypeNameList /* Vec<T>::Push */: DoubleColon Identifier UserTypeNameList;
/* 71 */ UserTypeNameList /* Vec<T>::New */: ;
/* 72 */ Regex: /\u{2f}(\\.|[^\\])*?\u{2f}/ : OwnedToken;
